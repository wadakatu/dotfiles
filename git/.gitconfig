[user]
        name = wadakatu
        email = wadakatukoyo330@gmail.com

[alias]
        st = status
        c  = commit

[init]
	    defaultBranch = main

[core]
        editor = vim
        ignorecase = false
        quotepath = false

[color]
        ui = auto

[push]
	    default = simple

[credential]
        helper = osxkeychain

[filter "lfs"]
        clean = git-lfs clean -- %f
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true